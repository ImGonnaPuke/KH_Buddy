<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#A5ADB3"
    tools:context=".kh2Menu">


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="397dp"
        android:layout_height="247dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_kh" />

    <Button
        android:id="@+id/summ"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="244dp"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Summons"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/skill"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Skills"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/summ" />

    <Button
        android:id="@+id/syth"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Syth Mats"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spells" />

    <Button
        android:id="@+id/spells"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="244dp"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Spells"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/drive"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Drives"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/key" />

    <Button
        android:id="@+id/arm"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Armor"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/drive" />

    <Button
        android:id="@+id/puzz"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Puzzle Pieces"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/acc" />

    <Button
        android:id="@+id/key"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Keyblades"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/skill" />

    <Button
        android:id="@+id/part"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Donald/Goofy"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/syth" />

    <Button
        android:id="@+id/acc"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:height="60dp"
        android:fontFamily="sans-serif-black"
        android:text="Accessories"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/part" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/adView"
        android:layout_width="408dp"
        android:layout_height="51dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="664dp"
        app:adSize="BANNER"
        app:adUnitId="ca-app-pub-7001225519001928/2420563521"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"></com.google.android.gms.ads.AdView>

</androidx.constraintlayout.widget.ConstraintLayout>